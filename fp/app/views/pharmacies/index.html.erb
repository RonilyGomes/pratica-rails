<p id="notice"><%= notice %></p>

<h1>Pharmacies</h1>

<table class="table">
  <thead>
    <tr align="center">
      <th>Nome</th>
      <th>Endereco</th>
      <th>Bairro</th>
      <th>Cep</th>
      <th>Cnpj farmacia</th>
      <th>Cnpj matriz</th>
      <th>Ano</th>
      <th>Mes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pharmacies.each do |pharmacy| %>
      <tr>
        <td><%= pharmacy.nome %></td>
        <td><%= pharmacy.endereco %></td>
        <td><%= pharmacy.bairro %></td>
        <td align="center"><%= pharmacy.cep %></td>
        <td align="center"><%= pharmacy.cnpj_farmacia %></td>
        <td align="center"><%= pharmacy.cnpj_matriz %></td>
        <td align="center"><%= pharmacy.ano %></td>
        <td align="center"><%= pharmacy.mes %></td>
        <td><%= link_to 'Show', pharmacy %></td>
        <td><%= link_to 'Edit', edit_pharmacy_path(pharmacy) %></td>
        <td><%= link_to 'Destroy', pharmacy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= paginate(@pharmacies) %>

<br>

<%= link_to 'New Pharmacy', new_pharmacy_path %>
